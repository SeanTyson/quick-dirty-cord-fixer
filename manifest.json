{
    "manifest_version": 3,
    "name": "Remove Elements",
    "version": "1.0",
    "description": "Remove HTML elements with class 'tf-v1-sidetab'",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["*://cord.co/*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
}